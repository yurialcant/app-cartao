  "clients": [
    {
      "clientId": "benefits-user-app",
      "enabled": true,
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "redirectUris": [
        "com.benefits.app://oauthredirect",
        "http://localhost:*"
      ],
      "webOrigins": [
        "*"
      ],
      "protocol": "openid-connect",
      "attributes": {
        "pkce.code.challenge.method": "S256"
      },
      "defaultClientScopes": ["openid", "profile", "email", "roles"]
    },
    {
      "clientId": "benefits-admin-portal",
      "enabled": true,
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "redirectUris": [
        "http://localhost:4200/*",
        "http://localhost:4200"
      ],
      "webOrigins": [
        "http://localhost:4200"
      ],
      "protocol": "openid-connect",
      "defaultClientScopes": ["openid", "profile", "email", "roles"]
    },
    {
      "clientId": "benefits-employer-portal",
      "enabled": true,
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "redirectUris": [
        "http://localhost:4201/*",
        "http://localhost:4201"
      ],
