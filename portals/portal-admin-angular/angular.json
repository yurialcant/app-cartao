{
  "$schema": "https://json.schemastore.org/angular-cli",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "portal-admin-angular": {
      "projectType": "application",
      "schematics": {},
      "root": ".",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/portal-admin-angular",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [],
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [{"type":"initial","maximumWarning":"2mb","maximumError":"5mb"}],
              "outputHashing": "all"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "portal-admin-angular:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "portal-admin-angular:build:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
